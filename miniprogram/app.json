{
  "pages": [
    "pages/index/index",
    "pages/list/list",
    "pages/myprofile/myprofile"
  ],
  "window": {
    "backgroundColor": "#F6F6F6",
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#ccc",
    "navigationBarTitleText": "随记",
    "navigationBarTextStyle": "white"
  },
  "tabBar": {
    "color": "#888",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "images/index.png",
        "selectedIconPath": "images/index_selected.png"
      },
      {
        "pagePath": "pages/list/list",
        "text": "列表",
        "iconPath": "images/list.png",
        "selectedIconPath": "images/list_selected.png"
      },
      {
        "pagePath": "pages/myprofile/myprofile",
        "text": "我的",
        "iconPath": "images/myprofile.png",
        "selectedIconPath": "images/myprofile_selected.png"
      }
    ]
  },
  "usingComponents": {
    "van-button": "/vantui/button/index",
    "van-cell": "/vantui/cell/index",
    "van-cell-group": "/vantui/cell-group/index",
    "van-icon": "/vantui/icon/index",
    "van-row": "/vantui/row/index",
    "van-col": "/vantui/col/index",
    "van-transition": "/vantui/transition/index",
    "van-checkbox": "/vantui/checkbox/index",
    "van-checkbox-group": "/vantui/checkbox-group/index",
    "van-datetime-picker": "/vantui/datetime-picker/index",
    "van-field": "/vantui/field/index",
    "van-rate": "/vantui/rate/index",
    "van-tab": "/vantui//tab/index",
    "van-tabs": "/vantui/tabs/index"
